var 
  x: int;
begin
  x := 1;
  if x = 1 then
    write 5
  else
    write -99;
  if x = 0 then
    write -99
  else
    write 4;
  if x < 1 then
    write -99
  else
    write 3;
  if x > 1 then
    write -99
  else
    write 2;
  if x <= 1 then
    write 1
  else
    write -99;
  if x >= 1 then
    write 0
  else
    write -99    
end
